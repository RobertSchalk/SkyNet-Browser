{
  "name": "skynetbrowser",
  "version": "1.0.0",
  "description": "Playing with electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset": "git reset --hard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobertSchalk/SkyNet-Electron-Browser.git"
  },
  "author": "Robert Schalk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RobertSchalk/SkyNet-Electron-Browser/issues"
  },
  "homepage": "https://github.com/RobertSchalk/SkyNet-Electron-Browserr#readme",
  "dependencies": {
    "bootstrap": "^3.4.1",
    "colors": "^1.3.3",
    "diff": "^4.0.1",
    "electromon": "^1.0.10",
    "electron": "^1.6.2",
    "electron-reload": "^1.5.0",
    "electron-window-state": "^5.0.3",
    "favicon-getter": "^1.1.3",
    "fs": "0.0.1-security",
    "jsonfile": "^2.4.0",
    "node": "^12.8.1",
    "path": "^0.12.7",
    "session": "^0.1.0",
    "tabs": "^0.2.0",
    "uuid": "^3.3.3",
    "webview": "0.0.1"
  },
  "devDependencies": {
    "electron": "^5.0.8"
  },
  "permissions": [
    "webview"
  ],
  "webview": {
    "partitions": [
      {
        "name": "static",
        "accessible_resources": [
          "header.html",
          "footer.html",
          "static.png"
        ]
      },
      {
        "name": "trusted*",
        "accessible_resources": [
          "local_*.html",
          "*.png",
          "*.js"
        ]
      },
      {
        "name": "trusted-audio",
        "accessible_resources": [
          "*.mp3"
        ]
      }
    ]
  }
}
